    <div class="contributor-1">
      <%= link_to "@tig-o", "https://github.com/tig-o"%>
    </div>

    <div class="contributor-2">
      <%= link_to "@philmarcu", "https://github.com/philmarcu"%>
    </div>

    <div class="contributor-3">
      <%= link_to "@mikekoul", "https://github.com/mikekoul"%>
    </div>

    <div class="contributor-4">
      <%= link_to "@jonathanmpope", "https://github.com/jonathanmpope"%>
    </div>

    <div class="contributor-5">
      <%= link_to "@alepbloyd", "https://github.com/alepbloyd"%>
